# Firebase Configuration
# Get these values from Firebase Console > Project Settings > General > Your apps > Web app config

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_DATABASE_URL=https://your_project_id-default-rtdb.firebaseio.com/
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456789012345

# Development Settings
NODE_ENV=development

# Optional: Google Analytics Measurement ID (if enabled)
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# ============================================
# AI Canvas Agent Configuration
# ============================================

# OpenAI API Key (REQUIRED)
VITE_OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# LangSmith Tracing (OPTIONAL - for debugging/monitoring)
VITE_LANGCHAIN_TRACING_V2=true
VITE_LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
VITE_LANGCHAIN_API_KEY=your_langsmith_api_key_here
VITE_LANGCHAIN_PROJECT=collabcanvas